select anncmt_time,earnings_date,company_name,symbol,consensus_eps,reported_eps,surprise_pct,raw_anncmt_time                                                                                                                                                                                                          from earning                                                                                                                                                                                                                                                                                                                     where surprise_pct > 0 AND earnings_date >= '2015-11-01' AND anncmt_time != 'NOT_SUPPLIED' order by surprise_pct desc limit 25
from earning
where surprise_pct > 0 AND earnings_date >= '2015-11-01' AND anncmt_time != 'NOT_SUPPLIED' order by surprise_pct desc limit 25